{
    "name": "essa/api_tool_kit",
    "description": "set of tools to build an api with laravel",
    "keywords": [
        "laravel",
        "api",
        "filters",
        "dynamic-pagination",
        "tools",
        "api-generator"
    ],
    "homepage": "https://github.com/ahmedesa/laravel-api-tool-kit",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Esa",
            "email": "ahmedesa6120@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.20.13|7.30.4|^8.22.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Essa\\APIToolKit\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Essa\\APIToolKit\\APIToolKitServiceProvider",
                "Essa\\APIToolKit\\MacroServiceProvider"
            ]
        }
    }
}
